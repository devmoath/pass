        "@babel/preset-env": "^7.14.8",
        "@babel/preset-typescript": "^7.14.5",
        "@types/jest": "^26.0.23",
        "babel-jest": "^27.0.6",
        "jest": "^27.0.6",
        "node-ray": "^1.16.0",
        "prettier": "^2.3.1",
        "typescript": "^4.3.5"
    },
    "keywords": [
        "password",
        "generator",
        "cli"
    ],
    "publishConfig": {
        "access": "public"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
