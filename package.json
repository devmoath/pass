{
    "name": "@devmoath/pass",
    "version": "0.1.0",
    "description": "Node.js cli to generate passwords",
    "license": "MIT",
    "author": "Moath <moath.alhajrii@gmail.com> (https://moath.dev/)",
    "homepage": "https://github.com/DevMoath/password-generator-cli#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DevMoath/password-generator-cli.git"
    },
    "bugs": {
        "url": "https://github.com/DevMoath/password-generator-cli/issues"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "preferGlobal": true,
    "bin": "./dist/index.js",
